{
	"name": "next-tailwind-template",
	"version": "0.1.0",
	"private": true,
	"description": "Next JS + Tailwind CSS Admin Template",
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"format:write": "prettier --write \"src/**/*.{css,js,json,jsx,ts,tsx}\"",
		"format": "prettier \"src/**/*.{css,js,json,jsx,ts,tsx}\"",
		"lint": "next lint",
		"prebuild": "prisma format && prisma generate",
		"predev": "prisma format && prisma generate",
		"dbpush": "npm run predev && prisma db push",
		"premigrate": "prisma db pull",
		"migrate": "prisma migrate dev --name initial-migration --create-only"
	},
	"dependencies": {
		"@headlessui/react": "^1.7.14",
		"@heroicons/react": "^2.0.18",
		"@next-auth/prisma-adapter": "^1.0.6",
		"@prisma/client": "^4.14.0",
		"@radix-ui/react-tooltip": "^1.0.5",
		"@tremor/react": "^2.5.0",
		"@types/node": "20.1.3",
		"@types/react": "18.2.6",
		"@types/react-dom": "18.2.4",
		"autoprefixer": "10.4.14",
		"eslint": "8.40.0",
		"eslint-config-next": "13.4.2",
		"next": "13.4.2",
		"next-auth": "^4.22.1",
		"postcss": "8.4.23",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-hot-toast": "^2.4.1",
		"react-hotkeys-hook": "^4.4.0",
		"tailwindcss": "3.3.2",
		"typescript": "5.0.4"
	},
	"devDependencies": {
		"@types/nodemailer": "^6.4.7",
		"nodemailer": "^6.9.2",
		"prettier": "^2.8.8",
		"prettier-plugin-tailwindcss": "^0.2.8",
		"prisma": "^4.14.0"
	}
}
